"NeoBundle"

"NeoBundle Settings"

set nocompatible
filetype off

if has('vim_starting')
  set runtimepath+=~/.vim/bundle/neobundle.vim
  call neobundle#begin(expand('~/.neobundle'))
endif

"NeoBundle Plugins"

NeoBundle 'Shougo/unite.vim' 

call neobundle#end()

"Settings"

set nocompatible
set number
set autoindent
set cindent
set hlsearch
set incsearch
set noswapfile

"Key"

set backspace=indent,eol,start
