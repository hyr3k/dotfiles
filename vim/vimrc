"NeoBundle"

"NeoBundle Settings"

set nocompatible
filetype off

if has('vim_starting')
  set runtimepath+=~/.vim/bundle/neobundle.vim
  call neobundle#rc(expand('~/.neobundle'))
endif

 "NeoBundle Plugins"
 

"Settings"

set nocompatible
set number
set autoindent
set cindent
set hlsearch
set incsearch
set noswapfile
